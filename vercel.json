{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ]
}
